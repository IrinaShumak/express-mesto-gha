{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb",
      "airbnb-base"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "linebreak-style": ["error", "unix"],
      "no-console": "off"
  }
}
